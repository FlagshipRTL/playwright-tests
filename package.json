{
  "name": "flagship-uat-playwright",
  "version": "1.0.0",
  "description": "Playwright UAT tests for FlagshipAI demand/supply validation",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "auth": "playwright codegen staging.flagshipai.com --save-storage=playwright/.auth/user.json",
    "report": "playwright show-report"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  }
}
